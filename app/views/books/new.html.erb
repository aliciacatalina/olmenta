<%= form_for @book, :url => books_path, :html => { :multipart => true } do |f| %>
  <%= f.label :title%>
  <%= f.text_field :title %>
  <%= f.label :author %>
  <%= f.text_field :author %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :cover%>
  <%= f.file_field :cover %>

  <%= f.submit "Submit" %>
<% end %>
